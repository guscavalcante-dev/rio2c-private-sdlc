@model PlataformaRio2C.Application.CQRS.Commands.UpdateNegotiation
@Html.HiddenFor(m => m.InitialBuyerOrganizationUid)
@Html.HiddenFor(m => m.InitialBuyerOrganizationName)
@Html.HiddenFor(m => m.InitialProjectUid)
@Html.HiddenFor(m => m.InitialProjectName)
@Html.HiddenFor(m => m.InitialNegotiationRoomConfigUid)
