@model MeetingParameterSearchViewModel
@{
    ViewBag.Title = Labels.OneToOneMeetings + " | " + Labels.Parameters;
}
<!--begin:: Quick Stats-->
<div class="row">
    <!--begin:: Total Count Widget-->
    <div class="col-sm-6">
        <div id="AudiovisualMeetingParametersTotalCountWidget" class="kt-portlet kt-portlet--fit kt-portlet--head-noborder" style="min-height: 230px;"></div>
    </div>
    <!--end:: Total Count Widget-->
    <!--begin:: Edition Count-->
    <div class="col-sm-6">
        <div id="AudiovisualMeetingParametersEditionCountWidget" class="kt-portlet kt-portlet--fit kt-portlet--head-noborder" style="min-height: 230px;"></div>
    </div>
    <!--end:: Edition Count-->
</div>
<!--end:: Quick Stats-->
@Html.Partial("Widgets/DataTablewidget", Model)
@section Styles {
    @Styles.Render("~/bundles/css/dataTables.css")
    @Styles.Render("~/bundles/css/select2.css")
}
@section Scripts {
    @Scripts.Render("~/bundles/js/dataTables.js")
    @Scripts.Render("~/bundles/js/chart.js")
    @Scripts.Render("~/bundles/js/select2.js")
    @Scripts.Render("~/bundles/js/bootbox.js")
    @Scripts.Render("~/bundles/js/audiovisual.meetingparameters.list.widget.js")
    @Scripts.Render("~/bundles/js/audiovisual.meetingparameters.editioncount.widget.js")
    @*@Scripts.Render("~/bundles/js/music.commissions.create.js")
    @Scripts.Render("~/bundles/js/music.commissions.update.js")
    @Scripts.Render("~/bundles/js/music.commissions.delete.js")*@
    <script>
        // Page translations ----------------------------------------------------------------------

        // Pagination -----------------------------------------------------------------------------
        var initialPage = @(Model.Page?.ToString() ?? "null");
        var initialPageSize = @(Model.PageSize?.ToString() ?? "null");

        jQuery(document).ready(function () {
            AudiovisualMeetingParametersTotalCountWidget.init();
            AudiovisualMeetingParametersEditionCountWidget.init();
            AudiovisualMeetingParametersDataTableWidget.init();
        });
    </script>
}