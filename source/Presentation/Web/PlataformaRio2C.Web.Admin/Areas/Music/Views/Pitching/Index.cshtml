@{
    ViewBag.Title = Labels.MusicCommission;
}
<!--begin:: Quick Stats-->
<div class="row">
    <!--begin:: Edition Scheduled Count Widget-->
    <div class="col-sm-6">
        <div id="MusicPitchingEditionScheduledCountWidget" class="kt-portlet kt-portlet--fit kt-portlet--head-noborder" style="min-height: 230px;"></div>
    </div>
    <!--end:: Edition Scheduled Count Widget-->
    <!--begin:: Edition Unscheduled Count Widget-->
    <div class="col-sm-6">
        <div id="MusicPitchingEditionUnscheduledCountWidget" class="kt-portlet kt-portlet--fit kt-portlet--head-noborder" style="min-height: 230px;"></div>
    </div>
    <!--end:: Edition Unscheduled Count Widget-->
</div>
<!--end:: Quick Stats-->
<div class="row">
    <!--begin:: Status Widget-->
    <div class="col-12">
        <div id="MusicPitchingStatusWidget" class="kt-portlet kt-portlet--fit kt-portlet--head-noborder" style="min-height: 75px;"></div>
    </div>
    <!--end:: Status Widget-->
</div>
@section Styles {
}
@section Scripts {
    @Scripts.Render("~/bundles/js/chart.js")
    @Scripts.Render("~/bundles/js/bootbox.js")
    @Scripts.Render("~/bundles/js/music.pitching.generate.widget.js")
    @Scripts.Render("~/bundles/js/music.pitching.editionscheduledcount.widget.js")
    @Scripts.Render("~/bundles/js/music.pitching.editionunscheduledcount.widget.js")
    <script>
        // Page translations ----------------------------------------------------------------------
        var translations = {
            distributeCommissionMembers: '@Labels.DistributeCommissionMembers'
        };

        // Pagination -----------------------------------------------------------------------------
        jQuery(document).ready(function () {
            MusicPitchingEditionScheduledCountWidget.init();
            MusicPitchingEditionUnscheduledCountWidget.init();
            MusicPitchingStatusWidget.init();
        });
    </script>
}