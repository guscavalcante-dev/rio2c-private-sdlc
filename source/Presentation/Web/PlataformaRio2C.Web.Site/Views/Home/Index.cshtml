@using PlataformaRio2C.Domain.Dtos
@{
    ViewBag.Title = string.Format(Labels.Welcome, (ViewBag.EditionDto as EditionDto)?.UrlCode);
}
<div class="row">
    <!--begin:Left panel-->
    <div class="col-md-4 col-lg-4">
        @{ Html.RenderPartial("Shared/_DashboardMenu"); }
    </div>
    <!--end:Left panel-->
    <!--begin:We Connect Widget-->
    <div class="col-md-8 col-lg-8">
        <div id="WeConnectWidget"></div>
    </div>
    <!--end:We Connect Widget-->
</div>
@section Styles {
}
@section Scripts {
    @Scripts.Render("~/bundles/js/weconnect.widget.js")
    <script>
        $(document).ready(function () {
            @if (ViewBag.ShowEmailSettings == true)
            {
                <text>AccountsEmailSettings.showUpdateModal();</text>
            }

            WeConnectWidget.init();
        });
    </script>
}