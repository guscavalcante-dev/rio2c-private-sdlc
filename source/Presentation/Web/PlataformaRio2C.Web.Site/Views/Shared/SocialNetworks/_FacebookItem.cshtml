@model string
<h3 class="kt-heading kt-heading--sm mb-0 mt-0">Facebook</h3>
<div class="row mt-3">
    <div class="col-12">
        @if (!string.IsNullOrEmpty(Model))
        {
            <a href="@Model.GetFacebookUrl()" class="btn btn-icon btn-circle btn-label-facebook" target="_blank">
                <i class="socicon-facebook"></i>
            </a>
            <a href="@Model.GetFacebookUrl()" target="_blank">@Model.</a>
        }
        else
        {
            @:-
        }
    </div>
</div>