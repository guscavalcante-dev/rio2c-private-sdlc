@model PlataformaRio2C.Application.CQRS.Commands.CreateTicketBuyerOrganization
<div class="modal fade" id="CreateCompanyInfoModal" role="dialog" aria-labelledby="CreateCompanyInfoModal" aria-hidden="true" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">
                    @(Labels.Create + " " + Labels.Informations)
                </h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                @using (Html.BeginForm("CreateCompanyInformation", "Executives", new { Area = "" }, FormMethod.Post, new { enctype = "multipart/form-data", id = "CreateCompanyInfoForm" }))
                {
                      @Html.HiddenFor(m => m.OrganizationUid)

                    <div id="form-container">
                        @Html.Partial("Modals/CreateCompanyInfoForm", Model)
                    </div>
                }
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary mr-auto" data-dismiss="modal">@Labels.Cancel</button>
                <button type="button" class="btn btn-primary" onclick="$('#CreateCompanyInfoForm').submit();">@Labels.Save</button>
            </div>
        </div>
    </div>
</div>